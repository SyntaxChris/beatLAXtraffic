<button id="land" style="position: absolute; z-index: 35; bottom: 0">LAND ZE PLANE</button>


<!-- QUESTIONS FORMAT -->
<div class="row" style="margin-top: 25px">
  <div class="large-12 medium-12 small-12 columns">

  <!-- questions header -->
    <div class="row" id="question-container">
      <div class="large-9 medium-9 small-12 large-centered medium-centered small-centered columns">
        <div id="question-header">
            <div id="main">What are you doing while you wait?</div>
            <div id="foot">Select all that apply</div>
        </div>
      </div>
    </div>
  <!-- end questions header -->

    <!-- tiles -->
    <div class="row">
      <div class="large-12 medium-12 small-12 large-centered medium-centered small-centered columns">
        <div class="row" >
          <div class="large-4 medium-4 small-4 columns">
              <%= render 'phone' %>
          </div>
          <div class="large-4 medium-4 small-4 columns">
              <%= render 'shop' %>
          </div>
          <div class="large-4 medium-4 small-4 columns">
              <%= render 'hangout' %>
          </div>
        </div>
        <div class="row">
          <div class="large-4 medium-4 small-4 columns">
            <%= render 'eat' %>
          </div>
          <div class="large-4 medium-4 small-4 columns">
            <%= render 'other' %>
          </div>
          <div class="large-4 medium-4 small-4 columns">
            <div class="row">
              <div class="large-12 medium-12 small-12 columns" style="padding-top: 5px">
                <div style="width: 100%; height: 170px; background-color: white; border-radius: 25px; padding:0">
                  <div class="arrow-left" style="position:absolute; z-index: 1;"></div>
                </div>
              </div>
            </div>
            
          </div>
        </div>
      </div>    
    </div>
    <!-- end tiles -->

  </div>
</div>
<!-- END QUESTION FORMAT -->

<!-- PARKING LOT -->
<div id="parking-lot">
  <%= render 'lines' %>
  <%= render 'cars' %>
</div>
<!-- END PARKING LOT -->

