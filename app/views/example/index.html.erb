<!-- QUESTIONS FORMAT -->
<div class="row" style="margin-top: 20px">
  <div class="large-12 medium-12 small-12 columns">

  <!-- questions header -->
    <div class="row" id="question-container">
      <div class="large-9 medium-9 small-12 large-centered medium-centered small-centered columns">
        <div id="question-header">
            <h3 id="main">What are you doing while you wait?</h3>
            <h5 id="foot">Select all that apply</h5> 
        </div>
      </div>
    </div>
  <!-- end questions header -->

    <!-- tiles 640px and up -->
    <div class="row" id="tiles-large">
      <div class="large-12 medium-12 small-12 large-centered medium-centered small-centered columns">
        <div class="row">
          <div class="large-4 medium-4 small-4 columns">
            <%= render 'phone' %>
            <div class="row" style="text-align: center; color: white">
              On my phone
            </div>
          </div>
          <div class="large-4 medium-4 small-4 columns">
            <%= render 'shop' %>
            <div class="row" style="text-align: center; color: white">
              Shop in the terminal
            </div>
          </div>
          <div class="large-4 medium-4 small-4 columns">
            <%= render 'hangout' %>
            <div class="row" style="text-align: center; color: white">
              Hang out
            </div>
          </div>
        </div>
        <div class="row" style="margin-top: 10px;">
          <div class="large-4 medium-4 small-4 columns">
            <%= render 'eat' %>
            <div class="row" style="text-align: center; color: white">
              Eat or drink
            </div>
          </div>
          <div class="large-4 medium-4 small-4 columns">
            <%= render 'other' %>
            <div class="row" style="text-align: center; color: white">
              Other
            </div>
          </div>
          <div class="large-4 medium-4 small-4 columns">
            <div class="row" style="padding:0">
              <div class="large-12 medium-12 small-12 columns">
                <div id="user-text-input">
                  <div class="arrow-left" style="position:absolute; z-index: 1;"></div>
                  <div class="row" style="text-align: center; color: #AA75D3;">
                    Please write here
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>    
    </div>
    <!-- end tiles 640px and up -->

    <!-- tiles 640px and down -->
    <div class="row" id="tiles-small">
      <div class="medium-10 small-11 medium-centered small-centered columns">

        <div class="row">
          <div class="medium-6 small-6 columns">
            <%= render 'phone' %>
            <div class="row small-tile-label" style="text-align: center; color: white">
              On my phone
            </div>
          </div>
          <div class="medium-6 small-6 columns">
            <%= render 'shop' %>
            <div class="row small-tile-label" style="text-align: center; color: white">
              Shop in the terminal
            </div>
          </div>
        </div>

        <div class="row" style="margin-top: 10px">
          <div class="medium-6 small-6 columns">
            <%= render 'hangout' %>
            <div class="row small-tile-label" style="text-align: center; color: white">
              Shop in the terminal
            </div>
          </div>
          <div class="medium-6 small-6 columns">
            <%= render 'eat' %>
            <div class="row small-tile-label" style="text-align: center; color: white">
              Shop in the terminal
            </div>
          </div>
        </div>

      </div>
    </div>
    <!-- end tiles 640px and down -->

  </div>
</div>
<!-- END QUESTION FORMAT -->


<!-- SCENES -->

<!-- parking lot -->
<div id="parking-lot">
  <%= render 'lines' %>
  <%= render 'cars' %>
</div>
<!-- end parking lot -->

<!-- END SCENES -->


<!-- DASHBOARD -->
<div id="dash">
  <div id="curve">
    <%= render 'curve' %>
  </div>
  <div id="dash-left"></div>
  <div id="dash-right"></div>
</div> 
 <!-- END DASHBOARD -->



<button id="land" style="position: absolute; z-index: 35; bottom: 0">LAND ZE PLANE</button>
