<div id="last-question-set">
  <!-- set 2 -->
  <div id="question-set-2">
    <div class="large-8 medium-8 small-12 larger-centered medium-centered small-centered columns">
      <h3 class="main-question">{{ currentNode.question.question }}</h3>
    </div>

    <div ng-repeat="answer in currentNode.answers">
      <div class="row" style="margin-bottom: 1.5%; margin-top: 3%">
        <div class="large-6 medium-6 small-9 large-centered medium-centered small-centered columns last-questions">
          <div class="question-wrapper-2 car-progress" ng-click="immediateSubmitAnswer(answer, false); advanceCar()">
            <div class="question-text-2" ng-id="{{ 'q' + $index + '-2' }}">
              {{ answer.answer }}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- end set 2 -->
</div>

