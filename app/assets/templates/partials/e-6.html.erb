<div id="last-question-set">
  <h3 class="main-question">{{ currentNode.question.question }}</h3>
  <div id="question-set-3">
    <div class="btn-wrapper">
        <button ng-repeat="answer in currentNode.answers" id="btn-{{$index}}" 
              class="question-3-btn car-progress" 
              ng-click="addAnswerToCurrentAnswers(currentNode.answers[{{$index}}])">
              {{ answer.answer }}
        </button>
    </div>
