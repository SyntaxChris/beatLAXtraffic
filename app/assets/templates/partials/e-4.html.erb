    <div id="last-question-set">
      <!-- set 1 -->
      <div id="question-set-1">
        <div class="large-8 medium-8 small-12 larger-centered medium-centered small-centered columns">
          <h3 class="main-question">{{ currentNode.question.question }}</h3>
        </div>

        <div ng-repeat="answer in currentNode.answers">
          <div ng-if="answer.answer.indexOf('Specify') === -1">
            <div class="row" style="margin-bottom: 1.5%">
              <div class="large-5 medium-5 small-9 large-centered medium-centered small-centered columns last-questions">
                <div class="question-wrapper-1" ng-click="immediateSubmitAnswer(answer)">
                  <div class="question-text" ng-id="{{ 'q' + $index }}">
                    {{ answer.answer }}
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div ng-if="answer.answer.indexOf('Specify') > -1">
            <div class="row" style="margin-bottom: 1.5%; margin-top: 3%">
              <div class="large-5 medium-5 small-9 large-centered medium-centered small-centered columns last-questions">
                <div class="question-wrapper-1-other" ng-click="immediateSubmitAnswer(answer)">
                  <div class="question-text" id="other-last">
                    {{ answer.answer }}
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

      <div id="last-questions-other-container" style="display: none">
        <div class="row" style="padding-top: 17px">
          <div class="large-10 medium-11 small-11 large-centered medium-centered small-centered columns">
            <textarea id="other-info-last-questions" cols="1" rows="5" maxlength="150"></textarea>
          </div>
        </div>
        <div class="row">
          <div class="large-6 medium-6 small-4 columns" style="text-align: center">
            <div id="char-count-last-questions" style="color: #B2C4D4"></div>
          </div>
          <div class="large-6 medium-6 small-8 columns">
            <div class="row">
              <div class="large-6 medium-6 small-6 columns">
                <div id="last-other-cancel">Cancel</div>
              </div>
              <div class="large-6 medium-6 small-6 columns" style="text-align: center">
                <div id="last-other-save" style="">Save</div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- end set 1 -->

    </div>
