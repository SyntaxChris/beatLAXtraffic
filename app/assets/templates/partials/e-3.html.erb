<!-- 14. TIMER PICKUP QUESTIONS -->
<div id="timer-pickup">
  <div class="content">
    <h3 class="main-question">
      You would have spent: 
    </h3>

    <div class="content-c">
      <div class="timer-c">
        <div class="timer-number" id="min">X</div>
        <div>min</div>
      </div>
      Picking up your passenger at the airport
    </div>


    <div class="content-c buttons-c">
      <div class="inner-question">
        How long does it typically take you to pick up your passenger at LAX?
      </div>
      <div>
         <button id="x-btn-timer-less" class="counter-btn">-</button>
         <button id="x-btn-timer-more" class="counter-btn">+</button> 
      </div>
      <div>
        <button ng-click="submit()" id="x-btn-timer-next">Next</button>
      </div>
    </div>
  </div>
</div>
<!-- END TIMER PICKUP QUESTIONS -->

